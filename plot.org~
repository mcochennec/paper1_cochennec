import matplotlib
import numpy as np
import matplotlib.pyplot as plt

matplotlib.use('Agg')
plt.figure(num=1,figsize=(10,4.8))
plt.clf()
matplotlib.rcParams['mathtext.fontset'] = 'stix'
matplotlib.rcParams['font.family'] = 'STIXGeneral'
plt.rc('legend',fontsize=12) # using a size in points
arr = np.array(data)
x = np.linspace(1e-4,1-1e-4)
anCapK12 = 2*(1-x)*(x+(1-x)*np.log(1-x))
#plot
plt.xlabel(r'$S_w$')
plt.ylabel(r'$\frac{K^*_{wo}}{K_0},\frac{\mu_o}{\mu_w}\frac{K^*_{ow}}{K_0}$')
plt.plot(arr[:,0],arr[:,1],'ko',markerfacecolor='red',ms=8,label=r'Num. (Rothman)')
plt.plot(arr[0:5,4],arr[0:5,5],'ks',markerfacecolor='blue',ms=8,label=r'Num. (Li et al.)')
plt.plot(arr[0:5,2],arr[0:5,3],'k^',markerfacecolor='green',ms=8,label=r'Exp. (Zarcone and Len.)')
plt.plot(arr[0:4,6],arr[0:4,7],'kD',markerfacecolor='yellow',ms=8,label=r'Exp. (Dullien and Dong)')
plt.plot(x,anCapK12,'k--',label=r'Ana. (Bacri)')
plt.ylim(0,0.3)
plt.xlim(0,1)
plt.legend(loc='upper left')

plt.tight_layout()
plt.savefig('littPerm.pdf')
'littPerm.pdf'
