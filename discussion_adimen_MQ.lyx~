\begin_body

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
0=-\nabla p+\mu(\phi)\left(\nabla^{2}\mathbf{u}-\frac{12}{\left(h\right)^{2}}\mathbf{u}\right)+\gamma(\frac{\pi}{4}\nabla\cdot\left(\frac{\boldsymbol{\nabla}\phi}{\vert\boldsymbol{\nabla}\phi\vert}\right)+\frac{2}{h})\delta(\phi)\mathbf{n},
\end{equation}

\end_inset

with
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathbf{n}=\frac{\boldsymbol{\nabla}\phi\,}{\left|\boldsymbol{\nabla}\phi\right|}\,\,;\,\,\,\delta(\phi)=6\left|\boldsymbol{\nabla}\phi\right|\left|\phi\left(1+\phi\right)\right|
\end{equation}

\end_inset

where I have taken 
\begin_inset Formula $\delta(\phi)$
\end_inset

 from Comsol implementation (to be checked)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbf{u}=\mathbf{u}'\times U_{r},\;p=p'\times\frac{\mu_{r}U_{r}}{l},\;\;\mathbf{x}=\mathbf{x}'\times l,
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
0=-\frac{\mu_{r}U_{r}}{l^{2}}\nabla'p'+\frac{\mu(\phi)U_{r}}{l^{2}}\left(\nabla'^{2}\mathbf{u}'-\frac{12}{\left(h/l\right)^{2}}\mathbf{u}'\right)+\frac{\gamma}{l^{2}}(\frac{\pi}{4}\nabla'\cdot\left(\frac{\boldsymbol{\nabla}'\phi}{\vert\boldsymbol{\nabla}'\phi\vert}\right)+\frac{2}{h/l})\delta'(\phi)\mathbf{n},
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
0=-\nabla'p'+\frac{\mu(\phi)}{\mu_{r}}\left(\nabla'^{2}\mathbf{u}'-\frac{12}{\left(h/l\right)^{2}}\mathbf{u}'\right)+\frac{\gamma}{\mu_{r}U_{r}}(\frac{\pi}{4}\nabla'\cdot\left(\frac{\boldsymbol{\nabla'}\phi}{\vert\boldsymbol{\nabla'}\phi\vert}\right)+\frac{2}{h/l})\delta'(\phi)\mathbf{n},
\end{equation}

\end_inset

with 
\begin_inset Formula $\delta'(\phi)=6\left|\boldsymbol{\nabla'}\phi\right|\left|\phi\left(1+\phi\right)\right|$
\end_inset


\end_layout

\end_body
