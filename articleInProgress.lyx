\begin_inset Formula 
\begin{equation}
\mathbf{K}_{w}=\mathbf{K}\mathbf{k}_{rw}(S_{w}),\qquad\mathbf{K}_{o}=\mathbf{K}\mathbf{k}_{ro}(S_{w}),
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula 
\begin{equation}
P_{c}(S_{w})=P_{o}-P_{w}.
\end{equation}

\end_inset


\end_layout

